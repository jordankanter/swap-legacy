


<rdf:RDF xmlns="http://example.com/swap/test/syntax/numbers.n3#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">

    <rdf:Description>
        <rdf:is xml:lang="fr">Le chat</rdf:is>
        <is>The french phrase 'Le chat'</is>
    </rdf:Description>

    <rdf:Description>
        <rdf:is rdf:datatype="http://example.com/types#rational">800/600</rdf:is>
        <is>800/600 with datatype http://example.com/types#rational</is>
    </rdf:Description>

    <rdf:Description>
        <rdf:is rdf:datatype="http://www.w3.org/2001/XMLSchema#double">2.0</rdf:is>
        <is>The number 2.0</is>
        <is>The number 2.0 expressed with extra trailing zeroes</is>
    </rdf:Description>

    <rdf:Description>
        <rdf:is rdf:datatype="http://www.w3.org/2001/XMLSchema#double">2000.0</rdf:is>
        <is>The double precision floating point 2.0e3</is>
    </rdf:Description>

    <rdf:Description>
        <rdf:is rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-2</rdf:is>
        <is>The integer -2</is>
    </rdf:Description>

    <rdf:Description>
        <rdf:is rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</rdf:is>
        <is>Zero</is>
    </rdf:Description>

    <rdf:Description>
        <rdf:is rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</rdf:is>
        <is>The integer 2</is>
        <is>The integer 2 expressed with leading zeroes</is>
    </rdf:Description>
</rdf:RDF>
