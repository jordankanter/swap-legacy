	echo "without removing libxpg4.dylib from DYLD_INSERT_LIBRARIES"
	echo "environment variable. So, this package should be uninstalled"
	echo "solely."

	done=no
	while [ "$done" != "yes" ]; do
		echo -n 'Are you sure? [y/N] '
		read answer
		case "$answer" in
			'Y'|'y') done=yes;;
			'N'|'n'|'') exit 1;;
			*) ;;
		esac
	done
fi
""";
