 (cd %d/usr/X11R6/lib && ln -s libGL.1.2.dylib libGL.1.dylib && ln -s libGL.1.2.dylib libGL.dylib)
 (cd %d/usr/X11R6/lib && cc -dynamiclib -o libGLU.1.3.dylib -install_name /usr/X11R6/lib/libGLU.1.dylib -compatibility_version 1.3 -current_version 1.3 -all_load libGLU.a -L. -L/usr/X11R6/lib -lGL)
