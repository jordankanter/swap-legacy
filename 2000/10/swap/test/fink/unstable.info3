    fink:compileScript """ perl Makefile.PL PREFIX=%p LIB='$(PREFIX)/lib/perl5' INSTALLMAN1DIR='$(PREFIX)/share/man/man1' INSTALLMAN3DIR='$(PREFIX)/share/man/man3'
 make
