# check for needed files
missing=
mcount=0
for file in bin/xterm bin/xrdb bin/rman \\
	    lib/libX11.dylib lib/libXpm.dylib lib/libXaw.dylib \\
	    include/X11/Xlib.h ; do
  if [ ! -f "/usr/X11R6/$file" ]; then
    missing="$missing /usr/X11R6/$file"
    mcount=$(($mcount+1))
  fi
