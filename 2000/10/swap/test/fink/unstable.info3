###
    fink:depends pkg:daemonic , pkg:libpoll_shlibs ;     fink:buildDepends pkg:libpoll , pkg:expat , pkg:tcltk_dev ; ###
    fink:source "http://%n.sourceforge.net/%n-%v.tar.gz";
