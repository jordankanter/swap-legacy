    :libxpg4_v20010605_r8     fink:compileScript """make CFLAGS='-DXPG4 -D__NETBSD_SYSCALLS -fno-common -no-cpp-precomp -flat_namespace'
""";
         fink:customMirror """Primary: http://www.ab.wakwak.com/~tino/darwin/
Secondary: http://hp.vector.co.jp/authors/VA008857/macosx/fink/mirror/libxpg4/
""";
         fink:descDetail """This is a port of FreeBSD 3.x's libxpg4, posted to darwin-development ML
(see http://www.darwinfo.org/devlist.php3?number=9143).
To use this, set DYLD_INSERT_LIBRARIES environment variable like this:
  setenv DYLD_INSERT_LIBRARIES /sw/lib/xpg4/libxpg4.dylib
If you use init.sh / init.csh, this will be done automatically.

They also set DYLD_FORCE_FLAT_NAMESPACE automatically. On Mac OS X 10.1,
this is required DYLD_INSERT_LIBRARIES to effect on a two-level namespace
images.
""";
         fink:descPackaging """This library should not be linked with any library or program. So,
library install location is outside of standard library search path.
""";
         fink:description "Locale-enabling preload library";
         fink:infoFrom <../../../../../../../sw/fink/dists/stable/main/finkinfo/libs/libxpg4-20010605-8.info>;
         fink:installScript """install -d -m 755 %i/lib/xpg4 %i/share %i/etc/profile.d
install -c -m 644 libxpg4.dylib %i/lib/xpg4
install -c -m 755 libxpg4.sh libxpg4.csh %i/etc/profile.d
mv ../locale %i/share/locale
""";
         fink:license "BSD";
         fink:maintainer  [
             con:firstName "Masanori";
             con:lastName "Sekino";
             con:mailbox <mailto:msek@users.sourceforge.net> ];
         fink:patchScript """sed 's|@PREFIX@|%p|g' <%a/%f.patch | patch -p1
""";
         fink:postInstScript """if [ -n "$DYLD_INSERT_LIBRARIES" ]; then
	DYLD_INSERT_LIBRARIES=`				\\
		echo $DYLD_INSERT_LIBRARIES		\\
		| sed 'y/:/\\n/'				\\
		| grep -v "^%p/lib/xpg4/libxpg4.dylib"	\\
		| paste -s -d ':' -`
fi
""";
         fink:preRmScript """if [ "$1" != 'upgrade' ]; then
	echo "**********WARNING**********"
	echo "If you uninstall libxpg4, you should restart your terminal and"
	echo "X Window System as soon as possible."
	echo "After you uninstall this package, you can't launch any program"
	echo "without removing libxpg4.dylib from DYLD_INSERT_LIBRARIES"
	echo "environment variable. So, this package should be uninstalled"
	echo "solely."

	done=no
	while [ "$done" != "yes" ]; do
		echo -n 'Are you sure? [y/N] '
		read answer
		case "$answer" in
			'Y'|'y') done=yes;;
			'N'|'n'|'') exit 1;;
			*) ;;
		esac
	done
fi
""";
         fink:revision "Masanori Sekino <msek@users.sourceforge.net>";
         fink:source "mirror:custom:%n-darwin-%v.tar.gz";
         fink:source2 "mirror:custom:localedata.tar.gz";
         fink:sourceDirectory "libxpg4" .
    
