fmtutil --byfmt pdfetex
fmtutil --byfmt pdflatex
fmtutil --byfmt pdfelatex
if [ -f %p/bin/mktexlsr ]; then %p/bin/mktexlsr %p/etc/texmf.local; fi
""";
         fink:postRmScript """if [ -f %p/bin/mktexlsr ]; then %p/bin/mktexlsr %p/share/texmf; %p/bin/mktexlsr %p/etc/texmf.local; fi
""";
