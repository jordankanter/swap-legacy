         fink:version "1.2" .
    
    :system_libgl_v1_2_r3     fink:conflicts :libgl;
         fink:descDetail """Install this package if you have manually installed the OpenGL libGL
and libGLU libraries in /usr/X11R6/lib.

You can use this if you have manually installed a recent CVS version
or binary test release of XFree86. If you have Xtools, you do not
need to use this package since system-xtools already checks for
libGL.
""";
         fink:description "Placeholder package for manually installed OpenGL libraries";
         fink:infoFrom <../../../../../../../sw/fink/dists/stable/main/finkinfo/x11-system/system-libgl-1.2-3.info>;
         fink:maintainer  [
             con:mailbox <mailto:fink-devel@lists.sourceforge.net> ];
         fink:preInstScript """
echo

# check for needed files
missing=
mcount=0
for file in lib/libGL.1.2.dylib lib/libGLU.1.3.dylib \\
            include/GL/gl.h include/GL/glu.h ; do
  if [ ! -f "/usr/X11R6/$file" ]; then
    missing="$missing /usr/X11R6/$file"
    mcount=$(($mcount+1))
  fi
done
if [ $mcount -ge 4 ]; then
  echo "You don't have OpenGL support installed. If you use the XDarwin 1.0a# binary"
  echo "test releases, you may have to install an additional tarball to get OpenGL"
  echo "support. If you have a version of XFree86 that doesn't support OpenGL, try"
  echo "installing the mesa package instead."
  echo
  echo "Press Return to continue."
  read
  exit 1
fi
if [ -n "$missing" ]; then
  echo "You have some OpenGL support installed, but there are files missing:"
  echo "$missing"
  echo "If you use the XDarwin 1.0a# binary test releases, you may have to install"
  echo "an additional tarball to get complete OpenGL support."
  echo
  echo "Press Return to continue."
  read
  exit 1
fi

echo "Valid OpenGL libraries and header files found."
echo

""";
         fink:provides "libgl";
         fink:revision "None <fink-devel@lists.sourceforge.net>";
         fink:type "bundle" .
    
