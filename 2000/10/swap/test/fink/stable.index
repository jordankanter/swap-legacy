 mv -f %i/bin/R %i/bin/R.orig
 rm -f %i/lib/R/bin/R
 sed -e "s|R_HOME_DIR=%i/lib/R|R_HOME_DIR=%p/lib/R|" < %i/bin/R.orig > %i/lib/R/bin/R 
 sed -e "s|R_HOME_DIR=%i/lib/R|R_HOME_DIR=%p/lib/R|" < %i/bin/R.orig > %i/bin/R 
 chmod 755 %i/lib/R/bin/R %i/bin/R
 rm -f %i/bin/R.orig
""";
