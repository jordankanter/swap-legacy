mkdir -p %i/share/texmf/web2c
install -p -m 755 texk/web2c/pdftex %i/bin
install -p -m 755 texk/web2c/pdfetex %i/bin
strip %i/bin/pdftex %i/bin/pdfetex
ln -s %p/bin/pdftex %i/bin/pdflatex
ln -s %p/bin/pdftex %i/bin/pdfinitex
ln -s %p/bin/pdftex %i/bin/pdfvirtex
ln -s %p/bin/pdfetex %i/bin/pdfelatex
ln -s %p/bin/pdfetex %i/bin/pdfeinitex
ln -s %p/bin/pdfetex %i/bin/pdfevirtex
install -p -m 755 texk/web2c/pdftex.pool %i/share/texmf/web2c
install -p -m 755 texk/web2c/pdfetex.pool %i/share/texmf/web2c
""";
