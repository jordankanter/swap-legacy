    :texi2html_v1_64_r1     fink:compileScript """ ./configure %c
 echo '#define DEFAULT_INFOPATH "%p/share/info:%p/info:/usr/local/share/info:/usr/local/lib/info:/usr/local/info:/usr/share/info:."' >>config.h
 make check
 make
""";
         fink:configureParams "--mandir=%p/share/man --infodir=%p/share/info";
         fink:conflicts :tetex_base;
         fink:depends :texinfo;
         fink:descDetail """Texinfo is the official documentation format of the GNU project. It
uses a single source file to produce both on-line information and
printed output. For more details about texinfo, see
http://www.texinfo.org. 

In contrast to the HTML produced by 'makeinfo --html' (the 'makeinfo'
program is part of the Texinfo distribution), the HTML output of
'texi2html' is highly configurable. Among others, with 'texi2html' you 
can customize your entire page layout (like headers, footers, style
sheets, etc), split documents at various levels and use 'latex2html' to
convert @tex sections.

'texi2html' should reasonably convert all Texinfo 4.0 constructs. If
not, please send a bug report to 'texi2html@mathematik.uni-kl.de'.
""";
         fink:descPort """Uses update-alternatives in case users also have texi2html installed via
tetex-base.
""";
         fink:description "Converts texinfo documents to HTML";
         fink:docFiles "README COPYING AUTHORS INTRODUCTION NEWS INSTALL ChangeLog TODO texi2html.texi";
         fink:homepage "http://www.mathematik.uni-kl.de/~obachman/Texi2html/";
         fink:infoFrom <../../../../../../../sw/fink/dists/unstable/main/finkinfo/text/texi2html-1.64-1.info>;
         fink:installScript """ make install prefix=%i mandir=%i/share/man infodir=%i/share/info
 mkdir -p %i/share/doc/%n/html
 mv %i/html/* %i/share/doc/%n/html
 rm -r %i/html
 mv %i/bin/texi2html %i/bin/texi2html.texi2html
 mv %i/share/man/man1/texi2html.1 %i/share/man/man1/texi2html.1.texi2html
""";
         fink:license "GPL";
         fink:maintainer  [
             con:firstName "Christian";
             con:lastName "Schaffner";
             con:mailbox <mailto:schaffner@artclip.ch> ];
         fink:postInstScript """update-alternatives --install %p/bin/texi2html texi2html %p/bin/texi2html.texi2html 60
update-alternatives --install %p/share/man/man1/texi2html.1 texi2html.1 %p/share/man/man1/texi2html.1.texi2html 60
""";
         fink:preRmScript """if [ $1 != "upgrade" ]; then
  update-alternatives --remove texi2html %p/bin/texi2html.texi2html
  update-alternatives --remove texi2html.1 %p/share/man/man1/texi2html.1.texi2html
fi
""";
         fink:revision "Christian Schaffner <schaffner@artclip.ch>";
         fink:source "http://www.mathematik.uni-kl.de/~obachman/Texi2html/Distrib/%n-%v.tar.gz" .
    
