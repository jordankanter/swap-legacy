         fink:descDetail """Leafnode is a news server suitable for small, limited-bandwidth sites
with only a few users. Leafnode keeps track of which groups are being
read, and downloads only articles in those groups. Leafnode has been
designed to require no maintenance and to be easy to set up.
""";
         fink:descPackaging "During installation a line for the server is added to /etc/inetd.conf";
         fink:description "Simple NNTP (usenet) server for small sites.";
         fink:docFiles "COPYING CREDITS ChangeLog FAQ README TODO fink/README-Fink";
         fink:homepage "http://www.leafnode.org";
         fink:infoFrom <../../../../../../../sw/fink/dists/unstable/main/finkinfo/net/leafnode-1.9.19-2.info>;
         fink:installScript "make install INSTALLROOT=%d";
         fink:license "BSD";
         fink:maintainer  [
             con:firstName "Christian";
             con:lastName "Swinehart";
             con:mailbox <mailto:cswinehart@mac.com> ];
         fink:patchScript "sed 's|@FINK_PREFIX@|%p|g' <%a/%f.patch | patch -p1";
         fink:postRmScript """# clean up and remove ourselves from inetd.conf
if [ $1 != "upgrade" ]; then
  cat /etc/inetd.conf | grep -v "leafnode" > /tmp/inetd.conf
  mv /tmp/inetd.conf /etc
  if [ -f /var/run/inetd.pid ]; then
          sudo kill -HUP `cat /var/run/inetd.pid`
  fi
  rm -rf %p/var/spool/news/*
fi
""";
         fink:preInstScript """if [ $1 != "upgrade" ]; then
 echo \\# leafnode: Leafnode server. Installed through fink at `date` >> /etc/inetd.conf
 echo nntp    stream  tcp     nowait  news    /usr/libexec/tcpd %p/sbin/leafnode >> /etc/inetd.conf
 echo \\# leafnode: end Leafnode section  >> /etc/inetd.conf
 sudo kill -HUP `cat /var/run/inetd.pid`
fi
""";
         fink:preRmScript """# Clear out the spool dirs
if [ $1 != "upgrade" ]; then
  rm -rf %p/var/spool/news/leaf.node/*
  rm -rf %p/var/spool/news/interesting.groups/*
  rm -rf %p/var/spool/news/message.id/*/*
  rm -f %p/var/spool/news/active.read
fi
""";
         fink:revision "http://www.leafnode.org";
         fink:setCFLAGS "-I%p/include -L%p/lib";
         fink:setCPPFLAGS "-no-cpp-precomp";
         fink:source "ftp://wpxx02.toxi.uni-wuerzburg.de:21/pub/%n-%v.tar.gz" .
    
