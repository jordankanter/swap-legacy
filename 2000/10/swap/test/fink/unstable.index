         fink:installScript """  mkdir -p %i/bin
  cp objs/run.ppc-darwin %i/bin/smlnj-runtime
  sed -e "s,@PREFIX@,%p," smlnj-script >! %i/bin/smlnj-script
  chmod 755 %i/bin/smlnj-script
""";
