    :mysql_v3_23_49_r1     fink:buildDepends :readline;
         fink:configureParams "--with-innodb --with-mysqld-user=mysql --without-readline --with-extra-charsets=complex --mandir=%p/share/man --infodir=%p/share/info --localstatedir=%p/var/mysql --libexecdir=%p/sbin --sysconfdir=%p/etc";
         fink:daemonicFile """<service>
<description>MySQL database server</description>
<message>MySQL database server</message>

<daemon name="mysqld">
<executable background="yes">%p/bin/safe_mysqld</executable>
</daemon>

</service>
""";
         fink:depends :daemonic,
                :dlcompat,
                :passwd,
                :readline_shlibs,
                :zlib;
         fink:descPackaging """Built with InnoDB transaction safe table support (http://www.innodb.com/).
You should install and update /sw/var/mysql/my.cnf in order to use InnoDB.

The MySQL benchmark suite is stowed away in <prefix>/share/mysql.

The InnoDB package revision by Andrei Verovski.
http://homepage.mac.com/macgurutemple/downloads.html

Previous package revisions by Christoph Pfisterer.
http://fink.sourceforge.net
""";
         fink:descPort """Supports Darwin out of the box, although the libtool scripts are
updated to build shared libraries. Version 3.23.47 fixes shutdown
problem on MacOS X/Darwin platform.
""";
         fink:descUsage """The package creates the administrative tables on installation. Be sure
to set a MySQL root password using mysqladmin:
'mysqladmin -u root password your-new-password' or
'mysqladmin -u root -h localhost -p password your-new-password'

You can start the MySQL server manually
1) using 'safe_mysqld --user=mysql &' - version 3.x
2) using 'mysqld_safe --user=mysql &' - version 4.x
or you can run 'daemonic enable mysql' to create a StartupItem for it.
All of these must be done as root.
""";
         fink:description "Open Source SQL database";
         fink:docFiles "README COPYING COPYING.LIB";
         fink:homepage "http://www.mysql.com/";
         fink:infoDocs "mysql.info";
         fink:infoFrom <../../../../../../../sw/fink/dists/unstable/main/finkinfo/database/mysql-3.23.49-1.info>;
         fink:installScript """ make install DESTDIR=%d benchdir_root=%p/share/mysql
 mkdir -p %i/var/mysql
 chmod 700 %i/var/mysql
 chown mysql %i/var/mysql
 mkdir -p %i/share/doc/%n
 cd Docs/; cp -R *.html Flags *.ps *.txt INSTALL-BINARY %i/share/doc/%n/
 cd %i/lib && ln -s mysql/libmysqlclient.la . && ln -s mysql/libmysqlclient.a . && ln -s mysql/libmysqlclient.dylib .
""";
         fink:license "GPL/LGPL";
         fink:maintainer  [
             con:firstName "Andrei";
             con:lastName "Verovski";
             con:mailbox <mailto:andrei.verovski@bigfoot.com> ];
         fink:postInstScript """# create initial database
if [ ! -d %p/var/mysql/mysql ]; then
  %p/bin/mysql_install_db --user=mysql
  chown -R mysql %p/var/mysql/mysql %p/var/mysql/test
fi

# update daemonic init script if necessary
daemonic install mysql
""";
         fink:preRmScript """# clean up
if [ $1 != "upgrade" ]; then
  daemonic remove mysql
fi
""";
         fink:revision "http://www.mysql.com/";
         fink:setCFLAGS "-O3 -fomit-frame-pointer";
         fink:setCXXFLAGS "-O3 -fomit-frame-pointer -felide-constructors -fno-exceptions -fno-rtti";
         fink:setLDFLAGS "-L%b/libmysql/.libs";
         fink:source "http://www.fh-wolfenbuettel.de/ftp/pub/database/mysql/Downloads/MySQL-3.23/%n-%v.tar.gz";
         fink:updateLibtool "true" .
    
