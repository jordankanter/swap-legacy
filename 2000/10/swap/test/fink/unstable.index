    :tex4ht_v20020226_r2     fink:buildDepends :expat,
                :freetype2,
                :gdbm,
                :gmp,
                :libjpeg,
                :libpng,
                :libtiff,
                :libxml2,
                :tcltk_dev;
         fink:compileScript """(cd doc/share; cc -O2 -o tex4ht tex4ht.c -DHAVE_DIRENT_H -DENVFILE='"%p/share/%n/tex4ht.env"' ) 
(cd doc/share; cc -O2 -o t4ht t4ht.c -DENVFILE='"%p/share/%n/tex4ht.env"' ) 
head -20 doc/share/t4ht.c > LICENSE
""";
         fink:customMirror """ nam-US: http://telia.dl.sourceforge.net/sourceforge/
 nam-US: http://unc.dl.sourceforge.net/sourceforge/
 eur-BE: http://belnet.dl.sourceforge.net/sourceforge/
""";
         fink:depends  [
             fink:option :ghostscript,
                    :ghostscript_nox ],
                :imagemagick,
                :tetex_base;
         fink:descDetail """TeX4ht is a highly configurable TeX-based authoring system for 
producing hypertext. It interacts with TeX-based applications 
through style files and postprocessors, leaving the processing 
of the source files to the native TeX compiler. Consequently, 
TeX4ht can handle the features of TeX-based systems in general, 
and of the LaTeX and AMS style files in particular. 
""";
         fink:descPort """There are no configure or Makefile scripts. 
Everything has to be done by hand.
TeX4ht needs a world writable directory for its bookkeeping file
tex4ht.fls. We put it into /var/tmp/tex4ht/.
""";
         fink:descUsage """The basic usage is "httex texfile.tex" or "htlatex latexfile.txt".
There is an html documentation at %p/share/doc/tex4ht/mn.html .
The htlatex, httex, and httexi scripts convert from LaTeX, TeX, and TeXinfo
to html.
Additional conversion scripts can be found in the directory %p/share/tex4ht.
""";
         fink:description "TeX to Hypertext translator";
         fink:docFiles "LICENSE lppl.txt";
         fink:homepage "http://www.cis.ohio-state.edu/~gurari/TeX4ht/mn.html";
         fink:infoFrom <../../../../../../../sw/fink/dists/unstable/main/finkinfo/text/tex4ht-20020226-2.info>;
         fink:installScript """mkdir -p %i/etc/texmf.local/tex/%n
mkdir -p %i/share/%n
mkdir -p %i/share/doc/%n
mkdir -p %i/bin
( cd doc/share; cp -p t4ht.c tex4ht.* *.txt test*.tex %i/share/%n/ )
( cd doc/share; cp -p all4ht/* tex4ht.sty %i/etc/texmf.local/tex/%n/ )
( cd doc/share; cp -pR ht-fonts %i/share/%n/ )
( cd doc/share; install -m 755 t*4ht %i/share/%n )
( cd doc/share; install -m 755 ht htlatex httex httexi %i/bin/ )
rm -rf doc/share doc/win95
cp -pR doc/* %i/share/doc/%n/
""";
         fink:license "OSI-Approved";
         fink:maintainer  [
             con:firstName "Martin";
             con:lastName "Costabel";
             con:mailbox <mailto:costabel@wanadoo.fr> ];
         fink:patchScript """sed 's|@T4HTDIR@|%p/share/%n|g' < %a/%f.patch | sed 's|@TFMDIR@|%p/share/texmf/fonts/tfm|g' | patch -p1
""";
         fink:postInstScript """if [ -f %p/bin/mktexlsr ]; then %p/bin/mktexlsr %p/etc/texmf.local; fi
mkdir -p /var/tmp/%n ; chmod 777 /var/tmp/%n
""";
         fink:postRmScript """if [ -f %p/bin/mktexlsr ]; then %p/bin/mktexlsr %p/etc/texmf.local; fi
rm -rf /var/tmp/%n 
""";
         fink:revision "http://www.cis.ohio-state.edu/~gurari/TeX4ht/mn.html";
         fink:source "mirror:custom:fink/%n-%v.zip";
         fink:source2 "http://www.latex-project.org/lppl.txt";
         fink:source2ExtractDir "%n";
         fink:sourceDirectory "%n" .
    
